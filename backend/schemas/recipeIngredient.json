{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/recipeIngredient.json",
    "type": "object",
    "properties": {
        "recipe_id": {
            "type": "integer"
        },
        "ingredient_id": {
            "type": "integer"
        },
        "quantity": {
            "type": "string",
            "maxLength": 25
        }
    },
    "required": [
        "recipe_id",
        "ingredient_id"
    ]
}