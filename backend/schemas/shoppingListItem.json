{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/shoppingListItem.json",
    "type": "object",
    "properties": {
        "item_id": {
            "type": "integer"
        },
        "list_id": {
            "type": "integer"
        },
        "ingredient_id": {
            "type": "integer"
        },
        "quantity": {
            "type": "string",
            "maxLength": 25
        },
        "checked": {
            "type": "boolean"
        }
    },
    "required": [
        "list_id",
        "ingredient_id",
        "quantity",
        "checked"
    ]
}